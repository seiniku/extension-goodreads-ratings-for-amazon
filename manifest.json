{
    "manifest_version": 2,
    "name": "Goodreads ratings for Chirp",
    "short_name": "Goodreads for Chirp",
    "description": "Show Goodreads ratings for books in Chirp.",
    "version": "1.7.3",
    "permissions": ["*://*.chirpbooks.com/*","https://www.goodreads.com/audiobooks/*"],
    "icons":
    {
        "128": "images/default-128.png"
    },
    "background":
    {
        "scripts": ["background.js"]
    },
    "content_scripts": [
    {
        "matches": ["*://*.chirpbooks.com/*"],
        "css": ["content.css"],
        "js": ["content.js"]
    }]
}